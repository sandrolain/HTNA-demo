{
  "name": "slot-demo",
  "version": "0.0.1",
  "description": "SLOT demo",
  "author": "Sandro Lain",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:coverage:open": "jest --coverage && open ./coverage/lcov-report/index.html",
    "build": "npx rollup -c",
    "watch": "npx rollup -cw",
    "serve": "npm run build && npx light-server -s . -p 5678 -o http://localhost:5678/demo -w \"dist/**/*.*\"",
    "prepare": "npx tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sandrolain/slot-demo.git"
  },
  "keywords": [
    "demo",
    "example"
  ],
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.8",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "happy-dom": "^0.12.1",
    "jest": "^25.2.4",
    "light-server": "^2.6.4",
    "node-sass": "^7.0.0",
    "open-cli": "^6.0.1",
    "rollup": "^2.3.2",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-postcss": "^2.5.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "^25.3.0",
    "typescript": "^3.8.3"
